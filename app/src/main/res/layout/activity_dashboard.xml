<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Dashboard">

    <de.hdodenhof.circleimageview.CircleImageView
        android:id="@+id/imgProfil"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_marginStart="10dp"
        android:layout_marginTop="10dp"
        android:src="@drawable/ic_profil" />

    <TextView
        android:id="@+id/tvUsername"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginTop="25dp"
        android:layout_toEndOf="@id/imgProfil"
        android:text="Bad Head "
        android:textSize="20dp"
        tools:ignore="HardcodedText,SpUsage" />

    <View
        android:id="@+id/vGaris"
        android:layout_width="fill_parent"
        android:layout_height="2dp"
        android:layout_below="@id/imgProfil"
        android:layout_marginStart="10dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="10dp"
        android:background="@color/black" />

    <com.google.android.material.imageview.ShapeableImageView
        android:id="@+id/imgWarning"
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:layout_below="@id/vGaris"
        android:layout_marginStart="10dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="10dp"
        android:src="@drawable/ic_warning"
        app:strokeColor="@color/black"
        app:strokeWidth="5dp" />

    <com.google.android.material.imageview.ShapeableImageView
        android:id="@+id/imgRokok"
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:layout_below="@id/vGaris"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="10dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="10dp"
        android:src="@drawable/ic_rokok"
        app:strokeColor="@color/black"
        app:strokeWidth="5dp" />

    <com.google.android.material.imageview.ShapeableImageView
        android:id="@+id/imgKirim"
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:layout_below="@id/imgRokok"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="10dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="10dp"
        android:src="@drawable/ic_kirim"
        app:strokeColor="@color/black"
        app:strokeWidth="5dp" />

    <com.google.android.material.imageview.ShapeableImageView
        android:id="@+id/imgUlang"
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:layout_below="@id/imgWarning"
        android:layout_marginStart="10dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="10dp"
        android:src="@drawable/ic_ulang"
        app:strokeColor="@color/black"
        app:strokeWidth="5dp" />

    <TextView
        android:id="@+id/Ninnama"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/imgUlang"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="30dp"
        android:text="5190311070_Raihatta Faisal Fachri"
        android:textSize="20dp"
        android:textStyle="bold"
        tools:ignore="HardcodedText,SpUsage" />


</RelativeLayout>